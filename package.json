{
  "name": "FantasticNativeComponents",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "build": "jest --verbose",
    "test": "jest --verbose",
    "test--w": "jest --watchAll --verbose",
    "test--c": "jest --verbose --coverage"
  },
  "dependencies": {
    "@types/lodash": "^4.14.121",
    "@types/styled-components": "^4.0.3",
    "apollo-boost": "^0.1.23",
    "aws-amplify": "^1.1.19",
    "aws-appsync": "^1.7.1",
    "aws-appsync-react": "^1.2.6",
    "graphql": "^14.0.2",
    "graphql-tag": "^2.10.0",
    "lodash": "^4.17.11",
    "react": "16.6.0-alpha.8af6728",
    "react-apollo": "^2.3.3",
    "react-native": "0.57.4",
    "react-native-easy-grid": "^0.2.0",
    "react-native-keyboard-aware-scroll-view": "^0.7.4",
    "react-native-snap-carousel": "^3.7.5",
    "react-native-vector-icons": "^6.1.0",
    "react-native-video": "^4.3.1",
    "react-navigation": "^2.18.2",
    "styled-components": "^4.0.3"
  },
  "devDependencies": {
    "@types/enzyme": "^3.1.14",
    "@types/enzyme-adapter-react-16": "^1.0.3",
    "@types/graphql": "^14.0.4",
    "@types/jest": "^23.3.9",
    "@types/react": "^16.4.18",
    "@types/react-native": "^0.57.7",
    "@types/react-native-snap-carousel": "^3.7.1",
    "@types/react-native-vector-icons": "^4.6.4",
    "@types/react-native-video": "^3.1.0",
    "@types/react-navigation": "^2.13.1",
    "@types/react-test-renderer": "^16.0.3",
    "@types/styled-components": "^4.0.3",
    "babel-eslint": "^10.0.1",
    "babel-jest": "23.6.0",
    "babel-plugin-inline-import": "^3.0.0",
    "babel-preset-react-native": "^4.0.1",
    "babel-preset-react-native-stage-0": "^1.0.1",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.6.0",
    "eslint": "^5.8.0",
    "eslint-plugin-react": "^7.11.1",
    "jest": "23.6.0",
    "jest-fetch-mock": "^1.6.6",
    "metro-react-native-babel-preset": "0.49.0",
    "react-addons-test-utils": "^15.6.2",
    "react-dom": "^16.6.0",
    "react-native-typescript-transformer": "^1.2.10",
    "react-test-renderer": "16.6.0-alpha.8af6728",
    "ts-jest": "^23.10.4",
    "typescript": "^3.1.6"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "js",
      "ts",
      "tsx",
      "jsx",
      "json",
      "node"
    ],
    "transform": {
      "^.+\\.(js)$": "<rootDir>/node_modules/react-native/jest/preprocessor.js",
      "\\.(ts|tsx)$": "<rootDir>/node_modules/ts-jest"
    },
    "globals": {
      "ts-jest": {
        "tsConfig": "tsconfig.jest.json"
      }
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules/(?!(jest-)?react-native|nodejs-formatter|styled-components|react-navigation)"
    ],
    "cacheDirectory": ".jest/cache",
    "modulePaths": [
      "<rootDir>"
    ],
    "setupFiles": [
      "./tests/setup.js"
    ]
  }
}
